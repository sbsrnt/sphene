{
  "name": "my-app",
  "version": "0.0.1",
  "author": "Your Name",
  "description": "React-TypeScript-Electron sample with Create React App and Electron Builder",
  "homepage": ".",
  "main": "electron/main.js",
  "private": true,
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "concurrently": "^5.2.0",
    "cypress": "^5.0.0",
    "cypress-intellij-reporter": "^0.0.4",
    "electron": "^9.0.0",
    "electron-builder": "^22.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "wait-on": "^5.0.0"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.9.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/history": "^4.7.6",
    "@types/jest": "25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/node": "14.0.6",
    "@types/react": "16.9.35",
    "@types/react-calendar": "^3.1.0",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "date-fns": "^2.14.0",
    "electron-context-menu": "^2.0.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "eslint": "^6.6.0",
    "flexboxgrid": "^6.3.1",
    "framer-motion": "^1.11.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-calendar": "^3.1.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-hook-form": "^6.3.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-toastify": "^6.0.8",
    "redux": "^4.0.5",
    "redux-axios-middleware": "^4.0.1",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.1",
    "typescript": "3.9.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "postinstall": "electron-builder install-app-deps",
    "dev": "concurrently \"SET BROWSER=none && yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p build/electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && electron-builder",
    "eject": "react-scripts eject",
    "cy": "cypress open",
    "db:seed": "yarn --cwd ../api-sphene run seed:run",
    "db:drop": "yarn --cwd ../api-sphene run schema:drop"
  },
  "build": {
    "appId": "sphene",
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "standard": {
    "env": [
      "browser",
      "mocha"
    ],
    "globals": [
      "cy",
      "Cypress",
      "expect",
      "assert"
    ]
  }
}
